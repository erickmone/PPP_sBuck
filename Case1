% ========================================================================
% Cascade Control for a Synchronous Buck Converter Design
% Inner Loop with PI via gamma-stability
% Total Loop with PIR via PPP
% Author : Erick Moreno-Negrete
% Institute 1: Centro de Investigacion y Estudios de Posgrado - FI-UASLP
% Institute 2: Centro de Electronica Industrial - ETSII-UPM
% Version : 2.6
% Case 1 Rnom = 1.5 Ohms
% ========================================================================

clc; close all

format longG

set(groot,'defaultAxesTickLabelInterpreter','latex');

%––– 1. Converter parameters ––––––––––––––––––––––––––––––––––––----------
p = struct('E',12, 'R',1.5, 'rL',879.75e-3, 'rS',7.5e-3,...
               'L',66e-6, 'Co',1980e-6);

%––– 2. Inner Transfer function Gid(s) ––––––––––––––––––––––––––––––––––––
[num, den] = gidCoeffs(p);

%––– 3. Gamma-stability selection ––––––––––––––––––––––––––––––––---------
gammaList = [0 -2000 -4000 -8000];           
w         = linspace(0,1e4,7000);          
colors    = lines(numel(gammaList));        
figure(1); clf;  hold on; grid on
xlabel('$k_p(\gamma,\omega)$','FontSize', 20, 'Interpreter','latex');
ylabel('$k_i(\gamma,\omega)$','FontSize', 20,'Interpreter','latex');
xlim([-0.1 0.14]);  ylim([-100 1000]);
curveHandles = gobjects(1, numel(gammaList));

for k = 1:numel(gammaList)
    gamma = gammaList(k);
    s     = gamma + 1i*w;
    Ns    = polyval(num,s);
    Ds    = polyval(den,s);

    % --- Stability Boundary ----------------------------------------------
    Kp = -real(Ds./Ns) - (gamma./w).*imag(Ds./Ns);
    Ki = (w + ((gamma.^2)./w)).*imag(Ds./Ns);
    curveHandles(k) = plot(Kp,Ki,'Color',colors(k,:),'LineWidth',1.8);
 
    % --- Auxiliar lines  k_i = -gamma*k_p - gamma*D/N  -------------------

    %––– 4. Polynomial evaluation at s = gamma (real-crossing)-------------
    D_sigma = polyval(den,gamma);        % d2*gamma^2 + d1*gamma + d0
    N_sigma = polyval(num ,gamma);       % n1*gamma   + n0

    %––– 5. Vector of k_p
    Kp_r = linspace(-0.5,0.7,1000);   
    %––– 6. Auxiliar lines of k_i
    Ki_r = -gamma*Kp_r - gamma*(D_sigma/N_sigma);
    plot(Kp_r,Ki_r,'--','Color',colors(k,:),'LineWidth',1.2)

    d2 = den(1);   d1 = den(2);   d0 = den(3);   % coeff. denominator
    n1 = num(1);   n0 = num(2);                  % coeff. numerator

    % --- 7. Intersection points ------------------------------------------
    if gamma ~= 0
        KpInt = (-d0*n0 - gamma*(2*d1*n0 + 3*n0*gamma + d1*n1*gamma ...
             + 2*n1*gamma^2)) / (n0 + n1*gamma)^2;
        KiInt =  gamma^2*(d1*n0 + 2*n0*gamma + n1*(-d0 + gamma^2)) ...
             / (n0 + n1*gamma)^2; 
        plot(KpInt,KiInt,'*k')
    end
    
end

% --- Label gamma-stability regions ---------------------------------------
legendLabels = {
    'Stability Boundary';
    '$-2000\leq\gamma<-4000$';
    '$-4000\leq\gamma<-8000$';
    '$\gamma\geq-8000$'
};
legend(curveHandles, legendLabels, 'Interpreter', 'latex', 'FontSize',...
       12, 'Location', 'northwest');

% --- 8. Roots of the closed-loop TF H_PIR  -------------------------------
KpDag = KpInt;
KiDag = KiInt;

numHPI = [KpDag*n1 (KiDag*n1)+(KpDag*n0) KiDag*n0];
denHPI = [1 (KpDag*n1)+d1 (KiDag*n1)+(KpDag*n0)+d0 KiDag*n0];
rPId=roots(denHPI);
rPIn=roots(numHPI);

alp = 1/(p.Co*p.R);
bta = 1/p.Co;
numOuter = bta;
denOuter = [1 alp];
rOd=roots(denOuter);
rOn=roots(numOuter);

a = KpDag*n1 + d1 + alp;
b = KiDag*n1 + KpDag*n0 + d0 + (KpDag*n1 + d1)*alp;
c = KiDag*n0 + (KiDag*n1 + KpDag*n0 + d0)*alp;
d = KiDag*n0*alp;

e = KpDag*n1*bta;
f = (KiDag*n1 + KpDag*n0)*bta;
g = KiDag*n0*bta;

numHPIR = [e f g];
denHPIR = [1 a b c d];
rPIRd = roots(denHPIR);
rPIRn = roots(numHPIR);

figure(2); clf;  hold on; grid on
xlabel('$\Re(s)$','FontSize', 20, 'Interpreter','latex');
ylabel('$\Im(s)$','FontSize', 20,'Interpreter','latex');
plot(real(rPIRn),imag(rPIRn),'ko','LineWidth',1.2)
plot(real(rPIRd),imag(rPIRd),'rx','LineWidth',1.2)

% --- 8. Election of the quadruple real-root  -----------------------------
s0 = [-4000 -3500 -3000];
s00 = -8000:100:0;

% --- Empty vectors for different elections of the controller -------------
Kp = zeros(1, length(s00));
Ki = zeros(1, length(s00));
Kr = zeros(1, length(s00));
H  = zeros(1, length(s00));

for i=1:length(s00)

    s=s00(i);

    % --- 9. Elimination produced polynomial solvability curve  -----------
    h1 = ((6 .* e .^ 3 .* s .^ 9) + (6 .* f .^ 3 .* s .^ 6) + (3 .* c .* g .^ 3) + sqrt((9 .* a .^ 2 .* e .^ 6 .* s .^ 16 + 60 .* a .* e .^ 6 .* s .^ 17 + 108 .* e .^ 6 .* s .^ 18 + 72 .* a .^ 2 .* e .^ 5 .* f .* s .^ 15 + 462 .* a .* e .^ 5 .* f .* s .^ 16 + 804 .* e .^ 5 .* f .* s .^ 17 + 72 .* a .^ 2 .* e .^ 5 .* g .* s .^ 14 + 252 .* a .^ 2 .* e .^ 4 .* f .^ 2 .* s .^ 14 + 480 .* a .* e .^ 5 .* g .* s .^ 15 + 1536 .* a .* e .^ 4 .* f .^ 2 .* s .^ 15 + 864 .* e .^ 5 .* g .* s .^ 16 + 2553 .* e .^ 4 .* f .^ 2 .* s .^ 16 + 528 .* a .^ 2 .* e .^ 4 .* f .* g .* s .^ 13 + 492 .* a .^ 2 .* e .^ 3 .* f .^ 3 .* s .^ 13 - 12 .* a .* b .* e .^ 5 .* g .* s .^ 13 + 12 .* a .* b .* e .^ 4 .* f .^ 2 .* s .^ 13 - 12 .* a .* c .* e .^ 5 .* f .* s .^ 13 + 12 .* a .* d .* e .^ 6 .* s .^ 13 + 3264 .* a .* e .^ 4 .* f .* g .* s .^ 14 + 2832 .* a .* e .^ 3 .* f .^ 3 .* s .^ 14 - 24 .* b .* e .^ 5 .* g .* s .^ 14 + 24 .* b .* e .^ 4 .* f .^ 2 .* s .^ 14 - 24 .* c .* e .^ 5 .* f .* s .^ 14 + 24 .* d .* e .^ 6 .* s .^ 14 + 5568 .* e .^ 4 .* f .* g .* s .^ 15 + 4440 .* e .^ 3 .* f .^ 3 .* s .^ 15 + 282 .* a .^ 2 .* e .^ 4 .* g .^ 2 .* s .^ 12 + 1614 .* a .^ 2 .* e .^ 3 .* f .^ 2 .* g .* s .^ 12 + 564 .* a .^ 2 .* e .^ 2 .* f .^ 4 .* s .^ 12 - 36 .* a .* b .* e .^ 4 .* f .* g .* s .^ 12 + 66 .* a .* b .* e .^ 3 .* f .^ 3 .* s .^ 12 - 30 .* a .* c .* e .^ 5 .* g .* s .^ 12 - 66 .* a .* c .* e .^ 4 .* f .^ 2 .* s .^ 12 + 66 .* a .* d .* e .^ 5 .* f .* s .^ 12 + 1728 .* a .* e .^ 4 .* g .^ 2 .* s .^ 13 + 9288 .* a .* e .^ 3 .* f .^ 2 .* g .* s .^ 13 + 3060 .* a .* e .^ 2 .* f .^ 4 .* s .^ 13 - 108 .* b .* e .^ 4 .* f .* g .* s .^ 13 + 144 .* b .* e .^ 3 .* f .^ 3 .* s .^ 13 - 36 .* c .* e .^ 5 .* g .* s .^ 13 - 144 .* c .* e .^ 4 .* f .^ 2 .* s .^ 13 + 144 .* d .* e .^ 5 .* f .* s .^ 13 + 3048 .* e .^ 4 .* g .^ 2 .* s .^ 14 + 14784 .* e .^ 3 .* f .^ 2 .* g .* s .^ 14 + 4476 .* e .^ 2 .* f .^ 4 .* s .^ 14 + 1800 .* a .^ 2 .* e .^ 3 .* f .* g .^ 2 .* s .^ 11 + 2568 .* a .^ 2 .* e .^ 2 .* f .^ 3 .* g .* s .^ 11 + 360 .* a .^ 2 .* e .* f .^ 5 .* s .^ 11 - 48 .* a .* b .* e .^ 4 .* g .^ 2 .* s .^ 11 + 96 .* a .* b .* e .^ 3 .* f .^ 2 .* g .* s .^ 11 + 144 .* a .* b .* e .^ 2 .* f .^ 4 .* s .^ 11 - 240 .* a .* c .* e .^ 4 .* f .* g .* s .^ 11 - 144 .* a .* c .* e .^ 3 .* f .^ 3 .* s .^ 11 + 48 .* a .* d .* e .^ 5 .* g .* s .^ 11 + 144 .* a .* d .* e .^ 4 .* f .^ 2 .* s .^ 11 + 10224 .* a .* e .^ 3 .* f .* g .^ 2 .* s .^ 12 + 13794 .* a .* e .^ 2 .* f .^ 3 .* g .* s .^ 12 + 1848 .* a .* e .* f .^ 5 .* s .^ 12 - 192 .* b .* e .^ 4 .* g .^ 2 .* s .^ 12 + 132 .* b .* e .^ 3 .* f .^ 2 .* g .* s .^ 12 + 318 .* b .* e .^ 2 .* f .^ 4 .* s .^ 12 - 450 .* c .* e .^ 4 .* f .* g .* s .^ 12 - 318 .* c .* e .^ 3 .* f .^ 3 .* s .^ 12 + 192 .* d .* e .^ 5 .* g .* s .^ 12 + 318 .* d .* e .^ 4 .* f .^ 2 .* s .^ 12 + 16536 .* e .^ 3 .* f .* g .^ 2 .* s .^ 13 + 20268 .* e .^ 2 .* f .^ 3 .* g .* s .^ 13 + 2496 .* e .* f .^ 5 .* s .^ 13 + 684 .* a .^ 2 .* e .^ 3 .* g .^ 3 .* s .^ 10 + 4500 .* a .^ 2 .* e .^ 2 .* f .^ 2 .* g .^ 2 .* s .^ 10 + 2100 .* a .^ 2 .* e .* f .^ 4 .* g .* s .^ 10 + 108 .* a .^ 2 .* f .^ 6 .* s .^ 10 - 36 .* a .* b .* e .^ 3 .* f .* g .^ 2 .* s .^ 10 + 600 .* a .* b .* e .^ 2 .* f .^ 3 .* g .* s .^ 10 + 132 .* a .* b .* e .* f .^ 5 .* s .^ 10 - 180 .* a .* c .* e .^ 4 .* g .^ 2 .* s .^ 10 - 732 .* a .* c .* e .^ 3 .* f .^ 2 .* g .* s .^ 10 - 132 .* a .* c .* e .^ 2 .* f .^ 4 .* s .^ 10 + 216 .* a .* d .* e .^ 4 .* f .* g .* s .^ 10 + 132 .* a .* d .* e .^ 3 .* f .^ 3 .* s .^ 10 + 3816 .* a .* e .^ 3 .* g .^ 3 .* s .^ 11 + 23616 .* a .* e .^ 2 .* f .^ 2 .* g .^ 2 .* s .^ 11 + 10728 .* a .* e .* f .^ 4 .* g .* s .^ 11 + 504 .* a .* f .^ 6 .* s .^ 11 + 12 .* b .^ 2 .* e .^ 4 .* g .^ 2 .* s .^ 10 - 24 .* b .^ 2 .* e .^ 3 .* f .^ 2 .* g .* s .^ 10 + 12 .* b .^ 2 .* e .^ 2 .* f .^ 4 .* s .^ 10 + 24 .* b .* c .* e .^ 4 .* f .* g .* s .^ 10 - 24 .* b .* c .* e .^ 3 .* f .^ 3 .* s .^ 10 - 24 .* b .* d .* e .^ 5 .* g .* s .^ 10 + 24 .* b .* d .* e .^ 4 .* f .^ 2 .* s