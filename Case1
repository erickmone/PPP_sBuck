    f3=figure(3);  hold on; grid on
    plot(s,h1,'*r')  
    grid on
    hold on
    plot(s,h2,'*b')
    xlabel('$s_0$','FontSize', 20, 'interpreter', 'latex');
    ylabel('$h$','FontSize', 20, 'interpreter', 'latex');
    ax = gca;
    ax.XAxisLocation = 'origin';
    ax.YAxisLocation = 'origin';
    ax.GridLineStyle = '--';
    ax.FontSize = 13; 
    axis tight;
    legend('$h_{1}$','$h_{2}$','FontSize', 13, 'interpreter', 'latex',...
           'location','best');
    set(f3,'Position',[12 12 600 250])
end

for i=1:length(s0)

    s=s0(i);

    % --- 10. PIR tuning rule  --------------------------------------------
